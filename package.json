{
  "name": "serverless-boilerplate",
  "version": "0.1.0",
  "description": "Serverless Boilerplate",
  "main": "handler.ts",
  "repository": "git@github.com:jgeschwendt/serverless-boilerplate.git",
  "author": "jlg <joshua@geschwendt.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "cross-env BABEL_ENV=production NODE_ENV=production sls package",
    "prisma:login": "cross-env prisma login",
    "deploy:api:dev": "tsc && cross-env BABEL_ENV=production NODE_ENV=production sls deploy --stage dev",
    "deploy:api:prod": "tsc && cross-env BABEL_ENV=production NODE_ENV=production sls deploy --stage prod",
    "deploy:prisma:dev": "cross-env prisma deploy --stage dev",
    "deploy:prisma:prod": "cross-env prisma deploy --stage prod",
    "start": "cross-env BABEL_ENV=development NODE_ENV=development sls offline --host 0.0.0.0 --port 4000",
    "tsc": "cross-env tsc"
  },
  "resolutions": {
    "graphql": "^0.12.3"
  },
  "dependencies": {
    "graphql": "^0.12.3",
    "graphql-tag": "^2.8.0",
    "graphql-yoga": "^1.4.3",
    "node-fetch": "^2.1.1",
    "prisma-binding": "^1.5.16"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@babel/preset-stage-0": "^7.0.0-beta.40",
    "@babel/preset-typescript": "^7.0.0-beta.40",
    "aws-sdk": "*",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-module-resolver": "^3.1.0",
    "circular-dependency-plugin": "^4.4.0",
    "cross-env": "^5.1.3",
    "graphql-import": "^0.4.5",
    "graphql-import-loader": "^0.2.0",
    "imports-loader": "^0.8.0",
    "jest": "^22.4.2",
    "npm-run-all": "^4.1.2",
    "prisma": "^1.3.5",
    "serverless": "^1.26.1",
    "serverless-offline": "^3.18.0",
    "serverless-webpack": "^5.0.0-rc.2",
    "typescript": "^2.7.2",
    "webpack": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  },
  "peerDependencies": {
    "aws-sdk": "*"
  }
}
